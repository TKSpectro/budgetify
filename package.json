{
  "name": "budgetify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d && next dev",
    "devi": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate-create-only": "prisma migrate dev --create-only"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@prisma/client": "^2.27.0",
    "apollo-server-micro": "^3.0.1",
    "bcrypt": "^5.0.1",
    "graphql": "^15.5.1",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "11.0.0",
    "nexus": "^1.1.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-operations": "1.18.3",
    "@graphql-codegen/typescript-react-apollo": "2.3.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.7",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^15.12.4",
    "@types/react": "17.0.11",
    "autoprefixer": "^10.2.6",
    "faker": "^5.5.3",
    "postcss": "^8.3.4",
    "prisma": "^2.27.0",
    "tailwindcss": "^2.1.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
