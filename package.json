{
  "name": "budgetify",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "scripts": {
    "dev": "docker-compose up -d db && next dev",
    "devi": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:codegen-watch": "graphql-codegen --config codegen.yml --watch",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate-create-only": "prisma migrate dev --create-only",
    "prisma:studio": "prisma studio --browser none",
    "prepare": "husky install",
    "test": "ts-mocha -p tests/tsconfig.json tests/**/*.ts"
  },
  "prisma": {
    "seed": "ts-node -O {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.4.0",
    "apollo-server-micro": "^3.4.1",
    "bcrypt": "^5.0.1",
    "chart.js": "^3.5.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.25.0",
    "graphql": "^15.6.0",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "^12.0.2",
    "next-i18next": "^8.10.0",
    "next-themes": "^0.0.15",
    "nexus": "^1.1.0",
    "nodemailer": "^6.7.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-chartjs-2": "^3.1.0",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/near-operation-file-preset": "^2.1.4",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.6",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/expect": "^24.3.0",
    "@types/faker": "^5.5.9",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@types/nodemailer": "^6.4.4",
    "@types/ps-tree": "^1.1.1",
    "@types/react": "^17.0.34",
    "@types/should": "^13.0.0",
    "@types/supertest": "^2.0.11",
    "autoprefixer": "^10.3.7",
    "chai": "^4.3.4",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "prisma": "^3.4.0",
    "ps-tree": "^1.2.0",
    "supertest": "^6.1.6",
    "tailwindcss": "^2.2.19",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "ws": "^8.2.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
