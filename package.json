{
  "name": "budgetify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d && next dev",
    "devi": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate-create-only": "prisma migrate dev --create-only",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node -O {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@heroicons/react": "^1.0.4",
    "@prisma/client": "^3.0.2",
    "apollo-server-micro": "^3.3.0",
    "bcrypt": "^5.0.1",
    "chart.js": "^3.5.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.24.0",
    "graphql": "^15.5.1",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "^11.1.2",
    "next-themes": "^0.0.15",
    "nexus": "^1.1.0",
    "nodemailer": "^6.6.3",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.15.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.8",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.9.2",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "17.0.21",
    "autoprefixer": "^10.3.4",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.4.1",
    "prisma": "^3.0.2",
    "tailwindcss": "^2.2.15",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
